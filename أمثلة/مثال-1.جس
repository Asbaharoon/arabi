*/


{
 لوحة = 0
  بينما (لوحة<10) {
    لوحة.اطبع(لوحة)
    لوحة++
}


{
  عرف اسم_آخر = 0
  افعل{
    لوحة.اطبع(اسم_آخر)
    اسم_آخر++
  } بينما (اسم_آخر<10) 
}


لوحة.اطبع('---------- اﻵ سنقوم بنفس اللشيء عن طريق حلقة "لكل"')؛

لكل ( ؛ ; ) (1+2)؛

ثابت هذه_المصفوفة = ['أنا محمد سمير'، 'أسعى لوضع لغتي  في مكانها الطبيعي']

// لكل: for of
لكل(عرف م من هذه_المفصفوفة) {
  لوحة.اطبع(م)؛
}

//  لكل: for in
لكل(عرف م داخل هذه_المفصفوفة) {
  لوحة.اطبع(م)؛
}



